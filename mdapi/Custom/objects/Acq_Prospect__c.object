    <validationRules>
        <fullName>Cannot_reopen_closed_Prospect</fullName>
        <active>false</active>
        <description>Once a prospect is closed, then analyst cannot reopen it</description>
        <errorConditionFormula>IF( AND(
				 ISPICKVAL(PRIORVALUE(Status__c),&quot;Cancelled / Dead&quot;),
				 TODAY() -	DATEVALUE(LastModifiedDate) &lt; 30
				) ,  
			true, 
			false)</errorConditionFormula>
        <errorMessage>Cannot be reopened</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_Validated</fullName>
        <active>true</active>
        <description>Property details should be validated.</description>
        <errorConditionFormula>AND(
     OR(
    				ISPICKVAL(Status__c, &apos;Underwriting Complete&apos;),
								ISPICKVAL(Status__c, &apos;Offer Approved&apos;)
				),
    
  
        Property_Validated__c = false
	    
				     
)</errorConditionFormula>
        <errorDisplayField>Property_Validated__c</errorDisplayField>
        <errorMessage>The property details should be validated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_fits_buy_box</fullName>
        <active>true</active>
        <description>Property details should be validated.</description>
        <errorConditionFormula>AND(
     OR(
    				ISPICKVAL(Status__c, &apos;Underwriting Complete&apos;),
								ISPICKVAL(Status__c, &apos;Offer Approved&apos;)
				),
    
        Property_fits_buy_box__c = false
				    
		)</errorConditionFormula>
        <errorDisplayField>Property_fits_buy_box__c</errorDisplayField>
        <errorMessage>The property should fit the buy box</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Lock</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISCHANGED(Record_Lock__c)), 
Record_Lock__c	= true 

)</errorConditionFormula>
        <errorMessage>Cannot edit this Record as it has either been Cancelled or Moved to Pipeline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Skipping</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 Offer_Accepted__c=False,
Converted_to_Pipeline__c= True 
)</errorConditionFormula>
        <errorDisplayField>Address__c</errorDisplayField>
        <errorMessage>Please complete the required steps before converting to Pipeline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Skipping_Steps</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 Offer_Accepted__c = false, 
Budget_Walk_Ordered__c = True 
)</errorConditionFormula>
        <errorDisplayField>Address__c</errorDisplayField>
        <errorMessage>A Budget Walk cannot be ordered until the Offered Amount has been accepted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Underwriting_Estimated_Complete</fullName>
        <active>true</active>
        <description>Underwriting Estimates complete</description>
        <errorConditionFormula>AND(
    OR(
    				ISPICKVAL(Status__c, &apos;Underwriting Complete&apos;),
								ISPICKVAL(Status__c, &apos;Offer Approved&apos;)
				),
    OR(
        ISNULL(Market_Value_UW__c),
								ISNULL(Initial_Rehab_Budget_Estimate__c),
								ISNULL(Expected_Gross_Monthly_Rent_UW__c),
								ISNULL(Property_Tax_Est_UW__c),
								ISNULL(Capitalized_Acq_Costs_Estimate__c)
								
	    ) 
				     
)</errorConditionFormula>
        <errorMessage>Underwriting estimate should be complete</errorMessage>
    </validationRules>
