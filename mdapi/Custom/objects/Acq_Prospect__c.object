    <fields>
        <fullName>Bath_Score__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
				Baths__c&lt;1,
				3,
				IF(
								AND(
												Baths__c&gt;=1,
												Baths__c&lt;2
								),
								2,
								IF(
												AND(
																Baths__c&gt;=2,
																Baths__c&lt;2.50
												),
												3,
												IF(
																AND(
																				Baths__c&gt;=2.50,
																				Baths__c&lt;3.50
																),
																4,
																IF(
																				AND(
																								Baths__c&gt;=3.50,
																								Baths__c&lt;=7
																				),
																				5,
																				5)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Bath Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
