    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Closing_Contract_Price</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				NOT(ISBLANK(Dir_IMG_Closing_Approval_Date__c )),
				ISCHANGED( Closing_Statement_Contract_Price__c )
				)</errorConditionFormula>
        <errorMessage>Closing Contract Price cannot be changed after closing approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closing_Contract_Price_Cannot_be_0</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
					ISPICKVAL(Pipeline_Status__c, &apos;Contract Reviewed&apos;),
	    Closing_Statement_Contract_Price__c &lt;=0
					)</errorConditionFormula>
        <errorMessage>Closing Contract Price should be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closing_Without_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				doDirectorClosingApproval__c =true,
   	ISPICKVAL( Pipeline_Status__c ,&apos;Review Complete&apos;)
				)</errorConditionFormula>
        <errorMessage>Cannot proceed to Closing Approval if due diligence approval is pending or not done</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_price_should_match_closing</fullName>
        <active>false</active>
        <description>Closing_Statement_Contract_Price__c should be equal to Contract_Price_After_Reduction__c</description>
        <errorConditionFormula>And(
				doDirectorClosingApproval__c = true,
				Closing_Statement_Contract_Price__c != Contract_Price_After_Reduction__c
)</errorConditionFormula>
        <errorMessage>Closing Contract Price should equal to Contract Price after reduction</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Diligence_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Pipeline_Status__c , &apos;Due Diligence Approved&apos;), 

OR( 
ISCHANGED( Final_Rehab_Budget_Est_UW__c), 

ISCHANGED( Prelim_Title_Reviewed_Approved__c), 


ISCHANGED( HOA_Estoppel_Approval_Date__c), 
ISCHANGED( Prelim_Title_Approval_Date__c), 
ISCHANGED( HOA_CC_R_Reviewed_and_Approved__c) 
) 
)</errorConditionFormula>
        <errorMessage>Due Diligence Approval is already done, cannot edit the existing values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMD</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
				
    OR( 
								doOrderEscrow__c =True,
        doDirectorClosingApproval__c =true, 
        doReview_Complete__c = True,				
							 doDirectorDueDiligenceApproval__c =true
				), 
				
 ISBLANK(Contract_Notes__c) 

)</errorConditionFormula>
        <errorMessage>Please review the contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Final_Budget_Missing</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
					doDirectorDueDiligenceApproval__c = True,
					ISNULL(Final_Rehab_Budget_Est_UW__c )
					)</errorConditionFormula>
        <errorMessage>Final Rehab Budget Estimate cannot be empty. Please complete the budget approval and reduction process first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Funds_Due_from_Buyer_Match</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
				doDirectorClosingApproval__c =true, 
				Funds_Due_From_Buyer__c  &lt;&gt; Closing_Statement_Funds_Due_from_Buyer__c
			)</errorConditionFormula>
        <errorMessage>Closing COE Amount should be equal to the COE Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HOA</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				doReview_Complete__c = True,
				ISPICKVAL( HOA_Y_N__c , &apos;Yes&apos;),
				ISBLANK( Verified_HOA_Name__c )
				)</errorConditionFormula>
        <errorMessage>Since HOA is present, please fill in the Verified HOA Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lease</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
doReview_Complete__c =True, 
Occupied_Status_Acquisition__c =&apos;Purchase Rented&apos;, 
Assumed_Lease_End_Date__c &lt;= Assumed_Lease_Start_Date__c)</errorConditionFormula>
        <errorMessage>The Lease End Date cannot be a date before the Lease Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Move_to_Queue</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISPICKVAL( Pipeline_Status__c ,&apos;Moved to Queue&apos;),
    ISBLANK(Dir_IMG_Closing_Approval_Date__c)	
				)</errorConditionFormula>
        <errorMessage>Cannot Move to Queue unless Closing Approval is complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Occupancy_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
					
				doReview_Complete__c = True,
				Occupied_Status_Acquisition__c =&apos;Purchase Rented&apos;,
				OR(
								ISNULL(Assumed_Lease_HAP_Rent__c), 
        ISNULL(Assumed_Lease_Tenant_Rent__c),
							 Assumed_Lease_Review_Required__c = False,
								ISBLANK(Assumed_Lease_End_Date__c), 
        ISBLANK(Assumed_Lease_Start_Date__c) 
						)
			)</errorConditionFormula>
        <errorMessage>Since Occupancy Status at Acquisition is Purchase Rented, please fill in all the required Lease details and also check the Lease Review Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Budget_Walk</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    OR( 
        doDirectorClosingApproval__c =true, 
        doReview_Complete__c = True,								
        doDirectorDueDiligenceApproval__c =true
				), 
				
   
					
				doOrderBudgetWalk__c = False
)</errorConditionFormula>
        <errorMessage>Budget Walk Record not found, please order a Budget Walk to proceed forward</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Complete</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
				OR(
         doDirectorClosingApproval__c =true, 
								 doOrderEscrow__c = false,
				     doDirectorDueDiligenceApproval__c =true),
				OR(
								
								ISPICKVAL( Pipeline_Status__c ,&apos;Escrow Ordered&apos;), 
								ISPICKVAL( Pipeline_Status__c ,&apos;Order Budget Walk&apos;))
								
)</errorConditionFormula>
        <errorMessage>Please Order an EMD and enter &quot;Review Complete&quot;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
