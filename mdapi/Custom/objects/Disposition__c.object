    <validationRules>
        <fullName>Bulk_Disposition_Group_Required</fullName>
        <active>true</active>
        <description>Bulk Disposition Group is Required only if Bulk Disposition is YES</description>
        <errorConditionFormula>AND( TEXT(Bulk_Disposition__c)== &apos;Yes&apos;, Bulk_Disposition_Group__c ==NULL)</errorConditionFormula>
        <errorDisplayField>Bulk_Disposition_Group__c</errorDisplayField>
        <errorMessage>Bulk Disposition Group is Required when  Bulk Disposition is YES</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_BX_Approval_of_Sale_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( BX_Approval_of_Sale__c, &apos;Yes&apos;),
       ISBLANK( Date_of_BX_Approval_of_Sale__c) 
 )</errorConditionFormula>
        <errorDisplayField>Date_of_BX_Approval_of_Sale__c</errorDisplayField>
        <errorMessage>You must enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_IH_Approval_of_Sale_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( IH_Approval_of_Sale__c, &apos;Yes&apos;),
       ISBLANK( Date_of_IH_Approval_of_Sale__c ) 
 )</errorConditionFormula>
        <errorDisplayField>Date_of_IH_Approval_of_Sale__c</errorDisplayField>
        <errorMessage>You must enter a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disposition_Date_required_when_Sold</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Settlement_Date__c) ,
    ISBLANK(Settlement_Date__c),
   !ISPICKVAL(Listing_Status__c,&apos;Cancelled&apos;),
   ISPICKVAL(Portfolio__r.Acquisition_Status__c, &apos;Sold&apos;)
   )</errorConditionFormula>
        <errorDisplayField>Settlement_Date__c</errorDisplayField>
        <errorMessage>Disposition date  must be populated When Portfolio Acquisition Status is Sold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Financial_Information_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Listing_Status__c, &apos;IH approved&apos;),
       OR(ISBLANK(BPO_As_Is__c) ,
           ISBLANK(Accounting_Basis_Actual__c),
           ISBLANK(Est_Current_Market_Value__c),         
           ISBLANK(IH_BPO_ARV__c),
           ISBLANK(Net_Proceeds_BPO__c),
           ISBLANK(Profit_Loss_BPO__c),
           ISBLANK(Profit_Loss_BPO_Percent__c),
           ISBLANK(Current_Market_Value__c),
           ISBLANK(Net_Proceeds_Market_Value__c),
           ISBLANK(Profit_Loss_Market_Value__c),
           ISBLANK(Profit_Loss_Market_Value_Percent__c),
           ISBLANK(Profit_Loss_Offer_Price__c),
           ISBLANK(X3rd_Party_BPO__c)          
       )
 )</errorConditionFormula>
        <errorMessage>Financial Information is mandatory. Please populate all Financial Information fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TRS_Transfer_Request_Required</fullName>
        <active>true</active>
        <description>TRS Transfer Request field will be required  for Fund 02 properties</description>
        <errorConditionFormula>AND( Portfolio__r.Fund__c ==&apos;02&apos; &amp;&amp;  (TEXT(TRS_Transfer_Request__c) == NULL ||TEXT(TRS_Transfer_Request__c) == &apos;&apos;) &amp;&amp;   ISPICKVAL(IH_Approval_of_Sale__c, &apos;Yes&apos;)  &amp;&amp; ISPICKVAL(BX_Approval_of_Sale__c, &apos;Yes&apos;))</errorConditionFormula>
        <errorDisplayField>TRS_Transfer_Request__c</errorDisplayField>
        <errorMessage>TRS Transfer Request field is required  for Fund 02 properties</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_IH_And_BX_Approvals</fullName>
        <active>true</active>
        <description>Once approval fields set to yes, no one should  be able to change it expect System Administrator</description>
        <errorConditionFormula>OR(AND(TEXT(PRIORVALUE(IH_Approval_of_Sale__c)) ==&apos;Yes&apos;,TEXT( IH_Approval_of_Sale__c)!=&apos;Yes&apos; ,$Profile.Name != &apos;System Administrator&apos;),AND(TEXT(PRIORVALUE(BX_Approval_of_Sale__c)) ==&apos;Yes&apos;,TEXT( BX_Approval_of_Sale__c)!=&apos;Yes&apos;,$Profile.Name != &apos;System Administrator&apos;))</errorConditionFormula>
        <errorMessage>Disposition has already been approved. Please contact your System Administrator</errorMessage>
    </validationRules>
