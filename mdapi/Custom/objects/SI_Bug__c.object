    <validationRules>
        <fullName>Assigned_to_can_only_move_defect_stage</fullName>
        <active>true</active>
        <errorConditionFormula>Assigned_To__r.Id = $User.Id &amp;&amp;
ISBLANK( Resolution__c ) &amp;&amp;
OR ( NOT(ISPICKVAL( Status__c , &quot;4. Ready for QA&quot;)), NOT(ISPICKVAL( Status__c , &quot;6. Rejected&quot;)),
NOT(ISPICKVAL( Status__c , &quot;7. Deferred&quot;)))</errorConditionFormula>
        <errorMessage>You are only able to move the Status to Ready for QA, Rejected or Deferred and you must populate the Resolution section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Description_How_to_Reproduce_Mandatory</fullName>
        <active>true</active>
        <description>THe how to reproduce field and description fields need to be mandatory</description>
        <errorConditionFormula>ISBLANK( Description__c ) || ISBLANK( How_To_Reproduce__c )</errorConditionFormula>
        <errorMessage>The How to Reproduce Field &amp; Description fields must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Bug_Resolution_Required</fullName>
        <active>true</active>
        <description>Forces the resolution to be populated before moving it to &quot;Fixed&quot;</description>
        <errorConditionFormula>OR(ISPICKVAL(Status__c, &apos;Fixed&apos;), ISPICKVAL(Status__c,&quot;4. Ready for QA&quot;)) &amp;&amp; ISBLANK(Resolution__c)</errorConditionFormula>
        <errorMessage>Please enter a Resolution when marking as &quot;Fixed&quot; or &quot;4. Ready for QA&quot;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
