    <validationRules>
        <fullName>COE_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
								NOT(ISBLANK(Dir_IMG_COE_Approval_Date__c)),
        ISCHANGED(COE_Amount__c)
								)</errorConditionFormula>
        <errorMessage>COE Amount cannot be changed after approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Receipt_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				 ISBLANK(Dir_IMG_COE_Approval_Date__c ),
				 COE_Receipt_Received__c = True
				)</errorConditionFormula>
        <errorMessage>Cannot order Receipt details unless the approval is complete</errorMessage>
    </validationRules>
