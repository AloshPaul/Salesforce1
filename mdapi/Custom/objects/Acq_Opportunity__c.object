        <picklistValues>
            <picklist>Stage__c</picklist>
            <values>
                <fullName>Closed</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Opportunity</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Pipeline</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Prospect</fullName>
                <default>false</default>
            </values>
        </picklistValues>
        <picklistValues>
            <picklist>status__c</picklist>
            <values>
                <fullName>Claimed</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Dead</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>New</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Submit for Underwriting</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Underwriting Complete</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Working</fullName>
                <default>false</default>
            </values>
        </picklistValues>
    </recordTypes>
    <searchLayouts>
        <excludedStandardButtons>New</excludedStandardButtons>
        <excludedStandardButtons>ChangeOwner</excludedStandardButtons>
        <excludedStandardButtons>Accept</excludedStandardButtons>
        <searchResultsAdditionalFields>APN__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Address__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>status__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Broker_Offer_Status__c</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Accepted_Contract_Price</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
					NOT(ISNULL(Offer_Accepted_Date__c)),
			 	NOT(ISNULL(PRIORVALUE( Accepted_Contract_Price__c))), 
					ISCHANGED( Accepted_Contract_Price__c )
					)</errorConditionFormula>
        <errorMessage>Cannot change the accepted contract price after its approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complete_Underwriting_Estimate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				OR( 
        ISBLANK( Broker_Market_Value__c ),
    				ISBLANK( Broker_Monthly_Rent__c ),
    				ISBLANK( Broker_Rehab__c ),
    				ISBLANK( Broker_Property_Taxes__c ), 
      		ISBLANK( Broker_Closing_Title_Fees_Est__c )
    ),
				OR( 
        ISPICKVAL(status__c, &apos;Submit For Underwriting&apos;), 
        doSubmitForUnderwriting__c = true 
    ) 
)</errorConditionFormula>
        <errorMessage>The underwriting estimates need to be completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HOA_name_cannot_be_empty</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISPICKVAL(HOA__c,&apos;Yes&apos;),
				ISBLANK(HOA_Name__c) 
)</errorConditionFormula>
        <errorMessage>HOA name cannot be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_details</fullName>
        <active>true</active>
        <errorConditionFormula>And(
				OR( 
        ISPICKVAL(Bid_Type__c, &apos;&apos;),
    				ISBLANK(  Year_Built__c  ), 
        ISBLANK( Bedrooms__c ), 
        ISBLANK( Baths__c  ), 
        ISBLANK( Square_Feet__c )			
    ),
    OR(
        ISPICKVAL(status__c, &apos;Underwriting Complete&apos;), 
					   doSubmitForUnderwriting__c = true
				)
)</errorConditionFormula>
        <errorMessage>The property details need to be completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Lock</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				 NOT(ISCHANGED(Record_Lock__c)),
		    	Record_Lock__c	= true
				
			)</errorConditionFormula>
        <errorMessage>Cannot edit a rejected Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SellerCounterOffer</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
  ISPICKVAL(Broker_Offer_Status__c, &apos;Counteroffer&apos;), 
  OR( 
  ISNULL(Seller_Counteroffer_Price__c) 
  ) 
)</errorConditionFormula>
        <errorMessage>Please fill the Seller Counteroffer Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Should_be_IH_Approved</fullName>
        <active>true</active>
        <description>Status should be submit for underwriting.
Broker offer status cannot be changed.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(status__c, &apos;Submit for Underwriting&apos;),
    ISNULL(Offer_Price__c),
     OR(
					    ISPICKVAL(Broker_Offer_Status__c, &apos;Submitted to Seller&apos;),
									ISPICKVAL(Broker_Offer_Status__c, &apos;Counteroffer&apos;),
									ISPICKVAL(Broker_Offer_Status__c, &apos;Accepted&apos;)
					) 				
)</errorConditionFormula>
        <errorMessage>Cannot change the status unless the offer is approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Submit_for_Underwriting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
        ISBLANK(  Broker_Offer_Price__c  ),
        ISPICKVAL(status__c, &apos;Submit For Underwriting&apos;)
)</errorConditionFormula>
        <errorDisplayField>Broker_Offer_Price__c</errorDisplayField>
        <errorMessage>Broker requested offer amount should be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_rule_for_square_feet</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Square_Feet__c &gt;9999 || Square_Feet__c &lt; 100)</errorConditionFormula>
        <errorMessage>Square feet should range between 100 and 9999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_rule_for_year</fullName>
        <active>true</active>
        <description>The Year should be in between 1800 and 2100</description>
        <errorConditionFormula>OR(VALUE(Year_Built__c ) &lt; 1800 ||VALUE(Year_Built__c ) &gt; 2100)</errorConditionFormula>
        <errorMessage>The year built should range between 1800 and 2100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>requiredField_Picklist</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Broker_Offer_Status__c, &apos;Accepted&apos;), 
OR( 
    ISNULL(Accepted_Contract_Price__c), 
    ISNULL(Accepted_Contract_Date__c), 
    ISBLANK(Broker_Contract_Comments__c),
    ISNULL(Escrow_Amount__c)

) 
)</errorConditionFormula>
        <errorMessage>Please fill in the IH Offer approval details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>tocheckBrokerOfferPrice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   
     Accepted_Contract_Price__c &gt; Offer_Price__c,
    OR(ISPICKVAL(Broker_Offer_Status__c, &apos;Accepted&apos;))

)</errorConditionFormula>
        <errorMessage>The accepted contract price should not be greater than the approved offer price</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
