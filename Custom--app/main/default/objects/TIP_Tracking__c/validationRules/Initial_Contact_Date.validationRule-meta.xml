<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Initial_Contact_Date</fullName>
    <active>true</active>
    <errorConditionFormula>RecordTypeId = '012E00000005gTA' &amp;&amp; Initial_Contact_Date__c > DATE(2013,04,30) &amp;&amp;  
ISPICKVAL(Initial_Contact_Result__c,"")</errorConditionFormula>
    <errorDisplayField>Initial_Contact_Time__c</errorDisplayField>
    <errorMessage>If Initial Contact Date is entered, Initial Contact Time, and Initial Contact Result should be entered also.</errorMessage>
</ValidationRule>
