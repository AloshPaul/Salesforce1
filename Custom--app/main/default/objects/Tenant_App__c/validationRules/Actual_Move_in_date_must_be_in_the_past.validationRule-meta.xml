<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Actual_Move_in_date_must_be_in_the_past</fullName>
    <active>true</active>
    <description>The "Actual Move-in Date" cannot be a future date</description>
    <errorConditionFormula>Actual_Move_in_Date__c > TODAY()</errorConditionFormula>
    <errorDisplayField>Actual_Move_in_Date__c</errorDisplayField>
    <errorMessage>The "Actual Move-in Date" cannot be a future date</errorMessage>
</ValidationRule>
