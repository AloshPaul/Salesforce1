<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_rule_for_square_feet</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Square_Feet__c >9999 || Square_Feet__c &lt; 100)</errorConditionFormula>
    <errorMessage>Square feet should range between 100 and 9999</errorMessage>
</ValidationRule>
